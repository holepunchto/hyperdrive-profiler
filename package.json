  "bugs": {
    "url": "https://github.com/holepunchto/hyperdrive-profiler/issues"
  },
  "homepage": "https://github.com/holepunchto/hyperdrive-profiler#readme",
  "devDependencies": {
    "standard": "^17.1.2"
  },
  "dependencies": {
    "bare-events": "^2.5.4",
    "bare-fs": "^4.0.1",
    "bare-os": "^3.4.0",
    "bare-path": "^3.0.0",
    "bare-performance": "^1.0.1",
    "corestore": "^7.0.0",
    "graceful-goodbye": "^1.3.3",
    "hypercore-id-encoding": "^1.3.0",
